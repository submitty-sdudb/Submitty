{
  "name": "submitty/submitty",
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/"
    }
  },
  "require": {
    "twig/twig": "^2.9.0",
    "ramsey/uuid": "^3.8.0",
    "lcobucci/jwt": "^3.2.5"
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1.3",
    "php-coveralls/php-coveralls": "^2.1.0"
  }
}
